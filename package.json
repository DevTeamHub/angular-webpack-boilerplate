  "scripts": {
    "clean": "rimraf public/bin",
    "debug": "npm run build:dev --debug=12345 ",
    "build:dev": "npm run clean && webpack --env=dev --config public/webpack.config.js",
    "build:qa": "npm run clean && webpack --env=qa --config public/webpack.config.js",
    "build:staging": "npm run clean && webpack --env=staging --config public/webpack.config.js",
    "build:prod": "npm run clean && webpack --env=prod --config public/webpack.config.js",
    "start": "webpack-dev-server --env=dev --config public/webpack.config.js --progress --profile --watch --content-base public/src/",
    "start:qa": "webpack-dev-server --env=qa --config public/webpack.config.js --progress --profile --watch --content-base public/src/",
    "start:staging": "webpack-dev-server --env=staging --config public/webpack.config.js --progress --profile --watch --content-base public/src/",
    "start:prod": "webpack-dev-server --env=prod --config public/webpack.config.js --progress --profile --watch --content-base public/src/"
  },
